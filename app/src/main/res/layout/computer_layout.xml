<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_of_main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="30dp"
    android:paddingRight="30dp"
    android:paddingTop="16dp"
    android:gravity="center"
    tools:context="com.example.pc.memory_test.Computer_activity"
    android:background="@drawable/gameback">

    <Button
        style="?android:attr/buttonStyleSmall"
        android:id="@+id/btnBabyBot"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="KOMPJUTERI ENIAC"
        android:background="@drawable/round_edges"
        android:textSize="25dp"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="15dp"/>

    <Button
        android:id="@+id/btnExpertBot"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="SUPER KOMPJUTERI"
        android:background="@drawable/round_edges"
        android:textSize="25dp"
        android:layout_below="@id/btnBabyBot"
        android:layout_marginBottom="15dp"/>




</RelativeLayout>
